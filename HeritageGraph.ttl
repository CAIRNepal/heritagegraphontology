heritageGraph:CasteGroup a owl:Class ;
    rdfs:label "CasteGroup" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty heritageGraph:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty heritageGraph:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty heritageGraph:english_description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty heritageGraph:traditional_role ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty heritageGraph:english_description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty heritageGraph:traditional_role ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty heritageGraph:traditional_role ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty heritageGraph:english_description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty heritageGraph:Name ] ;
    skos:definition "Hereditary social group (Jati) with specific ritual roles and occupational duties" ;
    skos:exactMatch crm:E74_Group ;
    skos:inScheme <nepaliCulturalHeritageOntology> .

