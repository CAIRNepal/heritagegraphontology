@prefix chlod: <https://cair-nepal.org/chlod> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<heritageGraph.owl.ttl> a owl:Ontology ;
    rdfs:label "heritageGraph" ;
    pav:version "1" ;
    skos:definition "A LinkML schema implementing CIDOC-CRM-aligned classes, event-centric patterns, and provenance for Nepal cultural heritage linked open data." .

chlod:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:EndDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Recurrence ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Recurrence ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:EndDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Recurrence ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:EndDate ] ;
    skos:exactMatch crm:E5_Event ;
    skos:inScheme <heritageGraph> .

chlod:HistoricalPeriod a owl:Class ;
    rdfs:label "Historical_Period" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:EndDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:EndDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:StartDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:EndDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ] ;
    skos:exactMatch crm:E4_Period ;
    skos:inScheme <heritageGraph> .

chlod:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Coordinates ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:SpacePrimitive ;
            owl:onProperty chlod:Coordinates ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Coordinates ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:ConditionState ] ;
    skos:exactMatch crm:E53_Place ;
    skos:inScheme <heritageGraph> .

chlod:TraditionOrPractise a owl:Class ;
    rdfs:label "Tradition_or_Practise" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Material ;
            owl:onProperty chlod:AssociatedMaterials ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:AssociatedMaterials ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ] ;
    skos:exactMatch crm:E28_Conceptual_Object ;
    skos:inScheme <heritageGraph> .

chlod:generatedAtTime a owl:DatatypeProperty ;
    rdfs:label "generatedAtTime" ;
    rdfs:range xsd:dateTime ;
    skos:definition "Date/time when the source or record was created." ;
    skos:inScheme <heritageGraph> .

chlod:wasAttributedTo a owl:ObjectProperty ;
    rdfs:label "wasAttributedTo" ;
    rdfs:range chlod:Actor ;
    skos:definition "Person or organization responsible for this source." ;
    skos:inScheme <heritageGraph> .

chlod:wasDerivedFrom a owl:ObjectProperty ;
    rdfs:label "wasDerivedFrom" ;
    rdfs:range chlod:Source ;
    skos:definition "Another source or entity from which this source was derived." ;
    skos:inScheme <heritageGraph> .

chlod:Source a owl:Class ;
    rdfs:label "Source" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:PublicationYear ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:URL_or_DigitalLink ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:URL_or_DigitalLink ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Actor ;
            owl:onProperty chlod:Authors ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty chlod:PublicationYear ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:PublicationYear ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Authors ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ] ;
    skos:exactMatch crm:E31_Document ;
    skos:inScheme <heritageGraph> .

chlod:AssociatedMaterials a owl:ObjectProperty ;
    rdfs:label "AssociatedMaterials" ;
    rdfs:range chlod:Material ;
    skos:definition "Physical objects used or associated with the tradition or practice." ;
    skos:inScheme <heritageGraph> .

chlod:Authors a owl:ObjectProperty ;
    rdfs:label "Authors" ;
    rdfs:range chlod:Actor ;
    skos:definition "Actor(s) responsible for creating the source." ;
    skos:inScheme <heritageGraph> .

chlod:Condition a owl:ObjectProperty ;
    rdfs:label "Condition" ;
    rdfs:range chlod:ConditionAssesment ;
    skos:definition "Records the condition of the artifact as determined by an assessment." ;
    skos:inScheme <heritageGraph> .

chlod:ConditionAssesment a owl:Class ;
    rdfs:label "ConditionAssesment" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:AssessedObject ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:AssessmentDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Actor ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:AssessedObject ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:AssessmentDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:AssessmentDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:CulturalEntity ;
            owl:onProperty chlod:AssessedObject ] ;
    skos:definition """The act of assessing the state of preservation of a Cultural_Entity.
""" ;
    skos:inScheme <heritageGraph> .

chlod:DigitalRepresentation a owl:ObjectProperty ;
    rdfs:label "DigitalRepresentation" ;
    rdfs:range chlod:VisualItem ;
    skos:definition "One or more visual/digital representations of the artifact." ;
    skos:inScheme <heritageGraph> .

chlod:Occupation a owl:Class,
        owl:DatatypeProperty ;
    rdfs:label "Occupation" ;
    rdfs:range xsd:string ;
    skos:definition "Occupation or role of the actor.",
        "Standardized types for actor roles or professions (e.g., painter, historian, sculptor)." ;
    skos:exactMatch crm:E55_Type ;
    skos:inScheme <heritageGraph> .

chlod:SpacePrimitive a owl:Class ;
    rdfs:label "SpacePrimitive" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:CRS ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:CRS ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:CRS ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Value ] ;
    skos:definition "Representation of spatial coordinates or geometry for a location." ;
    skos:exactMatch crm:E94_Space_Primitive ;
    skos:inScheme <heritageGraph> .

chlod:URL_or_DigitalLink a owl:DatatypeProperty ;
    rdfs:label "URL_or_DigitalLink" ;
    rdfs:range xsd:string ;
    skos:inScheme <heritageGraph> .

chlod:VisualItem a owl:Class ;
    rdfs:label "VisualItem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:FileURI ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:FileURI ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:FileURI ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:RepresentationOf ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:RepresentationOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:CulturalEntity ;
            owl:onProperty chlod:RepresentationOf ] ;
    skos:definition """Visual or digital representation of an artifact, such as a photograph, scan, or 3D model.
""" ;
    skos:exactMatch crm:E36_Visual_Item ;
    skos:inScheme <heritageGraph> .

chlod:AssessedObject a owl:ObjectProperty ;
    rdfs:label "AssessedObject" ;
    rdfs:range chlod:CulturalEntity ;
    skos:definition "The artifact whose condition is being assessed." ;
    skos:inScheme <heritageGraph> .

chlod:AssessmentDate a owl:DatatypeProperty ;
    rdfs:label "AssessmentDate" ;
    rdfs:range xsd:string ;
    skos:definition "Date or period of the assessment." ;
    skos:inScheme <heritageGraph> .

chlod:Assessor a owl:ObjectProperty ;
    rdfs:label "Assessor" ;
    rdfs:range chlod:Actor ;
    skos:definition "The person or entity performing the condition assessment." ;
    skos:inScheme <heritageGraph> .

chlod:BirthDate a owl:DatatypeProperty ;
    rdfs:label "BirthDate" ;
    rdfs:range xsd:string ;
    skos:definition "Birth date of the actor." ;
    skos:inScheme <heritageGraph> .

chlod:CRS a owl:DatatypeProperty ;
    rdfs:label "CRS" ;
    rdfs:range xsd:string ;
    skos:definition "Coordinate Reference System, e.g., \"EPSG:4326\"." ;
    skos:inScheme <heritageGraph> .

chlod:Coordinates a owl:ObjectProperty ;
    rdfs:label "Coordinates" ;
    rdfs:range chlod:SpacePrimitive ;
    skos:definition "Spatial representation (coordinates, geometry) defining the location." ;
    skos:inScheme <heritageGraph> .

chlod:DateCreated a owl:DatatypeProperty ;
    rdfs:label "DateCreated" ;
    rdfs:range xsd:string ;
    skos:definition "Date or period when the artifact was created." ;
    skos:inScheme <heritageGraph> .

chlod:DeathDate a owl:DatatypeProperty ;
    rdfs:label "DeathDate" ;
    rdfs:range xsd:string ;
    skos:definition "Death date of the actor." ;
    skos:inScheme <heritageGraph> .

chlod:FileURI a owl:DatatypeProperty ;
    rdfs:label "FileURI" ;
    rdfs:range xsd:string ;
    skos:definition "URI or file reference for the digital representation." ;
    skos:inScheme <heritageGraph> .

chlod:Mode a owl:DatatypeProperty ;
    rdfs:label "Mode" ;
    rdfs:range xsd:string ;
    skos:definition "Mode of representation (e.g., digitization, photo, 3D scan)." ;
    skos:inScheme <heritageGraph> .

chlod:PublicationYear a owl:DatatypeProperty ;
    rdfs:label "PublicationYear" ;
    rdfs:range xsd:integer ;
    skos:definition "Year the source was published." ;
    skos:inScheme <heritageGraph> .

chlod:Recurrence a owl:DatatypeProperty ;
    rdfs:label "Recurrence" ;
    rdfs:range xsd:string ;
    skos:definition "Recurrence pattern or frequency of the event." ;
    skos:inScheme <heritageGraph> .

chlod:RepresentationOf a owl:ObjectProperty ;
    rdfs:label "RepresentationOf" ;
    rdfs:range chlod:CulturalEntity ;
    skos:definition "The artifact that this visual item represents." ;
    skos:inScheme <heritageGraph> .

chlod:Unit a owl:DatatypeProperty ;
    rdfs:label "Unit" ;
    rdfs:range xsd:string ;
    skos:definition "Unit of measurement for the value" ;
    skos:inScheme <heritageGraph> .

chlod:Aliases a owl:DatatypeProperty ;
    rdfs:label "Aliases" ;
    rdfs:range xsd:string ;
    skos:definition "Alternative names, historical names, or nicknames." ;
    skos:inScheme <heritageGraph> .

chlod:CulturalEntity a owl:Class ;
    rdfs:label "Cultural_Entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chlod:Material ;
            owl:onProperty chlod:Material ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Material ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Dimension ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:DateCreated ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Aliases ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:DateCreated ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:VisualItem ;
            owl:onProperty chlod:DigitalRepresentation ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Aliases ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Condition ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:DateCreated ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:DigitalRepresentation ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Dimension ;
            owl:onProperty chlod:Dimension ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:ConditionAssesment ;
            owl:onProperty chlod:Condition ] ;
    skos:exactMatch crm:E18_Physical_Thing ;
    skos:inScheme <heritageGraph> .

chlod:Dimension a owl:Class,
        owl:ObjectProperty ;
    rdfs:label "Dimension" ;
    rdfs:range chlod:Dimension ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Unit ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Unit ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Unit ] ;
    skos:definition "Physical dimensions of the artifact",
        """Quantifiable property of an object, measured or approximated, e.g., length, height, weight.
""" ;
    skos:exactMatch crm:E54_Dimension ;
    skos:inScheme <heritageGraph> .

chlod:Actor a owl:Class ;
    rdfs:label "Actor" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:DeathDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:DeathDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Aliases ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:BirthDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Aliases ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:DeathDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Occupation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:BirthDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Occupation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:BirthDate ] ;
    skos:exactMatch crm:E39_Actor ;
    skos:inScheme <heritageGraph> .

chlod:EndDate a owl:DatatypeProperty ;
    rdfs:label "EndDate" ;
    rdfs:range xsd:string ;
    skos:definition "End date or time-span of the event." ;
    skos:inScheme <heritageGraph> .

chlod:Material a owl:Class,
        owl:ObjectProperty ;
    rdfs:label "Material" ;
    rdfs:range chlod:Material ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
