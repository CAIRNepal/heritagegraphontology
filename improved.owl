@prefix chlod: <https://cair-nepal.org/heritagegraph/> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<heritageGraph.owl.ttl> a owl:Ontology ;
    rdfs:label "heritageGraph" ;
    pav:version "3" ;
    skos:definition "A LinkML schema implementing CIDOC-CRM-aligned classes, event-centric patterns, and provenance for Nepal cultural heritage linked open data." .

chlod:DataGenerationEvent a owl:Class ;
    rdfs:label "DataGenerationEvent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:documented_source ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:KnowledgeRecord ;
            owl:onProperty chlod:generated_digital_entity ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty chlod:generation_time ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:data_derived_from ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:data_attributed_to ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:generated_digital_entity ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Source ;
            owl:onProperty chlod:documented_source ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Actor ;
            owl:onProperty chlod:data_attributed_to ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Source ;
            owl:onProperty chlod:data_derived_from ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:generated_digital_entity ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:generation_time ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:generation_time ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ] ;
    skos:definition """The activity of producing a digital record or data assertion (e.g., transcription, digitization, knowledge extraction) from a source document, ensuring PROV-O traceability.
""" ;
    skos:exactMatch crm:E7_Activity ;
    skos:inScheme <heritageGraph> .

<heritage:ConditionState/excellent> a owl:Class ;
    rdfs:label "excellent" ;
    rdfs:subClassOf chlod:ConditionState ;
    skos:definition "Fully intact with minimal wear" .

<heritage:ConditionState/fair> a owl:Class ;
    rdfs:label "fair" ;
    rdfs:subClassOf chlod:ConditionState ;
    skos:definition "Significant damage but major features intact" .

<heritage:ConditionState/fragmentary> a owl:Class ;
    rdfs:label "fragmentary" ;
    rdfs:subClassOf chlod:ConditionState ;
    skos:definition "Only fragments remain" .

<heritage:ConditionState/good> a owl:Class ;
    rdfs:label "good" ;
    rdfs:subClassOf chlod:ConditionState ;
    skos:definition "Minor damage, structurally stable" .

<heritage:ConditionState/poor> a owl:Class ;
    rdfs:label "poor" ;
    rdfs:subClassOf chlod:ConditionState ;
    skos:definition "Severe damage, structural instability" .

chlod:AssociatedMaterials a owl:ObjectProperty ;
    rdfs:label "AssociatedMaterials" ;
    rdfs:range chlod:Material ;
    skos:definition "Physical objects used or associated with the tradition or practice." ;
    skos:inScheme <heritageGraph> .

chlod:Authors a owl:ObjectProperty ;
    rdfs:label "Authors" ;
    rdfs:range chlod:Actor ;
    skos:definition "Actor(s) responsible for creating the source." ;
    skos:inScheme <heritageGraph> .

<https://cair-nepal.org/heritagegraph/CalendarSystem#gregorian> a owl:Class ;
    rdfs:label "gregorian" ;
    rdfs:subClassOf chlod:CalendarSystem ;
    skos:definition "Gregorian calendar" .

<https://cair-nepal.org/heritagegraph/CalendarSystem#nepali_calendar> a owl:Class ;
    rdfs:label "nepali_calendar" ;
    rdfs:subClassOf chlod:CalendarSystem ;
    skos:definition "Official Nepali calendar" .

<https://cair-nepal.org/heritagegraph/CalendarSystem#tibetan_calendar> a owl:Class ;
    rdfs:label "tibetan_calendar" ;
    rdfs:subClassOf chlod:CalendarSystem ;
    skos:definition "Tibetan lunar calendar" .

<https://cair-nepal.org/heritagegraph/CalendarSystem#vikram_samvat> a owl:Class ;
    rdfs:label "vikram_samvat" ;
    rdfs:subClassOf chlod:CalendarSystem ;
    skos:definition "Vikram Samvat (Nepal Sambat)" .

chlod:Condition a owl:ObjectProperty ;
    rdfs:label "Condition" ;
    rdfs:range chlod:ConditionAssesment ;
    skos:definition "Records the condition of the artifact as determined by an assessment." ;
    skos:inScheme <heritageGraph> .

chlod:ConditionAssesment a owl:Class ;
    rdfs:label "ConditionAssesment" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:CulturalEntity ;
            owl:onProperty chlod:AssessedObject ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Actor ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Assessor ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:AssessedObject ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:AssessedObject ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:ConditionState ;
            owl:onProperty chlod:ConditionState ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:DateExpression ;
            owl:onProperty chlod:AssessmentDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:AssessmentDate ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:AssessmentDate ] ;
    skos:definition "The act of assessing the state of preservation of a Cultural_Entity." ;
    skos:exactMatch crm:E14_Condition_Assessment ;
    skos:inScheme <heritageGraph> .

chlod:CreationEvent a owl:Class ;
    rdfs:label "CreationEvent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chlod:CulturalEntity ;
            owl:onProperty chlod:produced_cultural_entity ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:timespan ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:carried_out_by ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:HeritageCategory ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:produced_cultural_entity ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:timespan ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Location ;
            owl:onProperty chlod:took_place_at ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:produced_cultural_entity ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:TimeSpan ;
            owl:onProperty chlod:timespan ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Actor ;
            owl:onProperty chlod:carried_out_by ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:took_place_at ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Description ] ;
    skos:definition "An event representing the creation or production of a Cultural_Entity (E18)." ;
    skos:exactMatch crm:E12_Production ;
    skos:inScheme <heritageGraph> .

<https://cair-nepal.org/heritagegraph/DateCertainty#after> a owl:Class ;
    rdfs:label "after" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "No earlier than this date" .

<https://cair-nepal.org/heritagegraph/DateCertainty#before> a owl:Class ;
    rdfs:label "before" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "No later than this date" .

<https://cair-nepal.org/heritagegraph/DateCertainty#century> a owl:Class ;
    rdfs:label "century" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "Known to century precision" .

<https://cair-nepal.org/heritagegraph/DateCertainty#circa> a owl:Class ;
    rdfs:label "circa" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "Approximate date (± 10 years)" .

<https://cair-nepal.org/heritagegraph/DateCertainty#exact> a owl:Class ;
    rdfs:label "exact" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "Exact date known" .

<https://cair-nepal.org/heritagegraph/DateCertainty#period> a owl:Class ;
    rdfs:label "period" ;
    rdfs:subClassOf chlod:DateCertainty ;
    skos:definition "Known to historical period" .

<https://cair-nepal.org/heritagegraph/DeityType#avalokiteshvara> a owl:Class ;
    rdfs:label "avalokiteshvara" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "अवलोकितेश्वर (Avalokiteshvara)" .

<https://cair-nepal.org/heritagegraph/DeityType#buddha> a owl:Class ;
    rdfs:label "buddha" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "बुद्ध (Buddha)" .

<https://cair-nepal.org/heritagegraph/DeityType#durga> a owl:Class ;
    rdfs:label "durga" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "दुर्गा (Durga)" .

<https://cair-nepal.org/heritagegraph/DeityType#ganesh> a owl:Class ;
    rdfs:label "ganesh" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "गणेश (Ganesh)" .

<https://cair-nepal.org/heritagegraph/DeityType#shiva> a owl:Class ;
    rdfs:label "shiva" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "शिव (Shiva)" .

<https://cair-nepal.org/heritagegraph/DeityType#tara> a owl:Class ;
    rdfs:label "tara" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "तारा (Tara)" .

<https://cair-nepal.org/heritagegraph/DeityType#vishnu> a owl:Class ;
    rdfs:label "vishnu" ;
    rdfs:subClassOf chlod:DeityType ;
    skos:definition "विष्णु (Vishnu)" .

chlod:DigitalRepresentation a owl:ObjectProperty ;
    rdfs:label "DigitalRepresentation" ;
    rdfs:range chlod:VisualItem ;
    skos:definition "One or more visual/digital representations of the artifact." ;
    skos:inScheme <heritageGraph> .

<https://cair-nepal.org/heritagegraph/EthnicGroup#brahmin> a owl:Class ;
    rdfs:label "brahmin" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "ब्राह्मण (Brahmin)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#chhetri> a owl:Class ;
    rdfs:label "chhetri" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "क्षेत्री (Chhetri)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#gurung> a owl:Class ;
    rdfs:label "gurung" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "गुरुङ (Gurung)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#magar> a owl:Class ;
    rdfs:label "magar" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "मगर (Magar)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#newar> a owl:Class ;
    rdfs:label "newar" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "नेवार (Newar)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#sherpa> a owl:Class ;
    rdfs:label "sherpa" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "शेर्पा (Sherpa)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#tamang> a owl:Class ;
    rdfs:label "tamang" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "तामाङ (Tamang)" .

<https://cair-nepal.org/heritagegraph/EthnicGroup#tharu> a owl:Class ;
    rdfs:label "tharu" ;
    rdfs:subClassOf chlod:EthnicGroup ;
    skos:definition "थारू (Tharu)" .

<https://cair-nepal.org/heritagegraph/HeritageCategory#immovable_site> a owl:Class ;
    rdfs:label "immovable_site" ;
    rdfs:subClassOf chlod:HeritageCategory ;
    skos:definition "Tangible immovable heritage (e.g., temple, stupa, palace)" .

<https://cair-nepal.org/heritagegraph/HeritageCategory#intangible_practice> a owl:Class ;
    rdfs:label "intangible_practice" ;
    rdfs:subClassOf chlod:HeritageCategory ;
    skos:definition "Intangible cultural heritage (e.g., ritual, knowledge)" .

<https://cair-nepal.org/heritagegraph/HeritageCategory#mixed_heritage> a owl:Class ;
    rdfs:label "mixed_heritage" ;
    rdfs:subClassOf chlod:HeritageCategory ;
    skos:definition "Mixed tangible and intangible" .

<https://cair-nepal.org/heritagegraph/HeritageCategory#movable_artifact> a owl:Class ;
    rdfs:label "movable_artifact" ;
    rdfs:subClassOf chlod:HeritageCategory ;
    skos:definition "Tangible movable heritage (e.g., statue, manuscript)" .

<https://cair-nepal.org/heritagegraph/HeritageCategory#natural_site> a owl:Class ;
    rdfs:label "natural_site" ;
    rdfs:subClassOf chlod:HeritageCategory ;
    skos:definition "Natural heritage site" .

chlod:KnowledgeRecord a owl:Class ;
    rdfs:label "Knowledge_Record" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:documented_in ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:Source ;
            owl:onProperty chlod:documented_in ] ;
    skos:definition "An asserted statement, fact, or entity record produced by the DataGenerationEvent." ;
    skos:exactMatch crm:E73_Information_Object ;
    skos:inScheme <heritageGraph> .

chlod:Occupation a owl:Class,
        owl:ObjectProperty ;
    rdfs:label "Occupation" ;
    rdfs:range chlod:Type ;
    skos:definition "Occupation or role of the actor.",
        "Standardized types for actor roles or professions (e.g., painter, historian, sculptor)." ;
    skos:exactMatch crm:E55_Type ;
    skos:inScheme <heritageGraph> .

chlod:SpacePrimitive a owl:Class ;
    rdfs:label "SpacePrimitive" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:CRS ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:CRS ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:CRS ] ;
    skos:definition "Representation of spatial coordinates or geometry for a location." ;
    skos:exactMatch crm:E94_Space_Primitive ;
    skos:inScheme <heritageGraph> .

chlod:URL_or_DigitalLink a owl:DatatypeProperty ;
    rdfs:label "URL_or_DigitalLink" ;
    rdfs:range xsd:string ;
    skos:inScheme <heritageGraph> .

chlod:VisualItem a owl:Class ;
    rdfs:label "VisualItem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:RepresentationOf ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:FileURI ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:FileURI ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:RepresentationOf ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty chlod:Mode ],
        [ a owl:Restriction ;
            owl:allValuesFrom chlod:CulturalEntity ;
            owl:onProperty chlod:RepresentationOf ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty chlod:FileURI ] ;
    skos:definition "Visual or digital representation of an artifact, such as a photograph, scan, or 3D model." ;
    skos:exactMatch crm:E36_Visual_Item ;
    skos:inScheme <heritageGraph> .

chlod:associated_cultural_entity a owl:ObjectProperty ;
    rdfs:label "associated_cultural_entity" ;
    rdfs:range chlod:CulturalEntity ;
    skos:inScheme <heritageGraph> .

chlod:associated_event a owl:ObjectProperty ;
    rdfs:label "associated_event" ;
    rdfs:range chlod:Event ;
    skos:inScheme <heritageGraph> .

chlod:associated_tradition a owl:ObjectProperty ;
    rdfs:label "associated_tradition" ;
    rdfs:range chlod:TraditionOrPractice ;
    skos:inScheme <heritageGraph> .

chlod:associated_with a owl:ObjectProperty ;
    rdfs:label "associated_with" ;
    rdfs:range chlod:CulturalAssociation ;
    skos:inScheme <heritageGraph> .

chlod:created a owl:ObjectProperty ;
    rdfs:label "created" ;
    rdfs:range chlod:CulturalEntity ;
    skos:inScheme <heritageGraph> .

chlod:current_location a owl:ObjectProperty ;
    rdfs:label "current_location" ;
    rdfs:range chlod:Location ;
    skos:inScheme <heritageGraph> .

chlod:custody_transferred_from a owl:ObjectProperty ;
    rdfs:label "custody_transferred_from" ;
    rdfs:range chlod:CustodyTransferEvent ;
    skos:definition "The custody transfer event this Actor participated in as the relinquishing party." ;
    skos:inScheme <heritageGraph> .

chlod:custody_transferred_to a owl:ObjectProperty ;
    rdfs:label "custody_transferred_to" ;
    rdfs:range chlod:CustodyTransferEvent ;
    skos:definition "The custody transfer event this Actor participated in as the gaining party." ;
    skos:inScheme <heritageGraph> .

chlod:data_attributed_to a owl:ObjectProperty ;
    rdfs:label "data_attributed_to" ;
    rdfs:range chlod:Actor ;
    skos:definition "The Actor responsible for generating this piece of data/digital record (curator, transcriber, data extraction agent)." ;
    skos:inScheme <heritageGraph> .

chlod:data_derived_from a owl:ObjectProperty ;
    rdfs:label "data_derived_from" ;
    rdfs:range chlod:Source ;
    skos:definition "The Source (E31) from which the digital record or assertion was derived." ;
    skos:inScheme <heritageGraph> .

chlod:depicts_deity a owl:ObjectProperty ;
    rdfs:label "depicts_deity" ;
    rdfs:range chlod:DeityType ;
    skos:definition "The deity or conceptual entity depicted by the artifact or site." ;
    skos:inScheme <heritageGraph> .

chlod:documented_source a owl:ObjectProperty ;
    rdfs:label "documented_source" ;
    rdfs:range chlod:Source ;
    skos:definition "The original Source (E31) that was documented by the DataGenerationEvent (E7)." ;
    skos:inScheme <heritageGraph> .

chlod:documents_actor a owl:ObjectProperty ;
    rdfs:label "documents_actor" ;
    rdfs:range chlod:Actor ;
    skos:inScheme <heritageGraph> .

chlod:documents_cultural_entity a owl:ObjectProperty ;
    rdfs:label "documents_cultural_entity" ;
    rdfs:range chlod:CulturalEntity ;
    skos:inScheme <heritageGraph> .

chlod:documents_event a owl:ObjectProperty ;
    rdfs:label "documents_event" ;
    rdfs:range chlod:Event ;
    skos:inScheme <heritageGraph> .

chlod:documents_historical_period a owl:ObjectProperty ;
    rdfs:label "documents_historical_period" ;
    rdfs:range chlod:HistoricalPeriod ;
    skos:inScheme <heritageGraph> .

chlod:documents_tradition a owl:ObjectProperty ;
    rdfs:label "documents_tradition" ;
    rdfs:range chlod:TraditionOrPractice ;
    skos:inScheme <heritageGraph> .

chlod:event_involves a owl:ObjectProperty ;
    rdfs:label "event_involves" ;
    rdfs:range chlod:Actor ;
    skos:inScheme <heritageGraph> .

chlod:has_custodian a owl:ObjectProperty ;
    rdfs:label "has_custodian" ;
    rdfs:range chlod:Actor ;
    skos:definition "The Actor (person, Guthi, institution) who has current custody of the entity." ;
    skos:inScheme <heritageGraph> .

chlod:has_practice_type a owl:ObjectProperty ;
    rdfs:label "has_practice_type" ;
    rdfs:range chlod:TraditionOrPractice ;
    skos:inScheme <heritageGraph> .

chlod:hosted_event a owl:ObjectProperty ;
    rdfs:label "hosted_event" ;
    rdfs:range chlod:Event ;
    skos:inScheme <heritageGraph> .

chlod:includes_cultural_entity a owl:ObjectProperty ;
    rdfs:label "includes_cultural_entity" ;
    rdfs:range chlod:CulturalEntity ;
    skos:inScheme <heritageGraph> .

chlod:includes_event a owl:ObjectProperty ;
    rdfs:label "includes_event" ;
    rdfs:range chlod:Event ;
    skos:inScheme <heritageGraph> .

chlod:includes_person a owl:ObjectProperty ;
    rdfs:label "includes_person" ;
    rdfs:range chlod:Actor ;
    skos:inScheme <heritageGraph> .

chlod:mentioned_in a owl:ObjectProperty ;
    rdfs:label "mentioned_in" ;
    rdfs:range chlod:Source ;
    skos:inScheme <heritageGraph> .

chlod:occurs_at a owl:ObjectProperty ;
    rdfs:label "occurs_at" ;
    rdfs:range chlod:Location ;
    skos:inScheme <heritageGraph> .

chlod:participated_in a owl:ObjectProperty ;
